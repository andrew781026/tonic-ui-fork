{
  "name": "consumer-tonic-ui",
  "version": "0.0.2",
  "description": "tonic-ui for tailwind version",
  "main": "index.js",
  "scripts": {
    "prebase": "postcss --config src/base src/base/*.css --base src --dir dist",
    "base": "node utils/mergeFile.js 'dist/base/*.css' 'dist/base.css'",
    "postbase": "node utils/cssToObj.js dist/base.css dist/base.js",
    "precomponents": "postcss --config src/components src/components/**/*.scss --base src --dir dist",
    "components": "node utils/mergeFile.js 'dist/components/*.scss' 'dist/component.css'",
    "postcomponents": "node utils/cssToObj.js dist/component.css dist/component.js",
    "cp:lib": "node utils/cp.js --src src/lib --dest dist/lib",
    "cp:colors": "node utils/cp.js --src src/colors --dest dist/colors",
    "cp:root": "node utils/cp.js --src src/index.js --dest dist/index.js",
    "gen:colors": "node utils/colorGenerator.js",
    "build": "npm run base && npm run components && npm run gen:colors && npm run cp:colors && npm run cp:lib && npm run cp:root",
    "doc:build": "npm run build && cd astro && npm run build",
    "dev": "npm run build && cd astro && npm run dev",
    "lint": "eslint src/**/*.js",
    "publish": "node publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrew781026/tonic-ui-fork.git"
  },
  "keywords": [],
  "author": "andrew_wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrew781026/tonic-ui-fork/issues"
  },
  "homepage": "https://github.com/andrew781026/tonic-ui-fork#readme",
  "dependencies": {
    "color": "^4.2.3",
    "consumer-tonic-design-system": "git+ssh://git@adc.github.trendmicro.com:Consumer-Frontend/consumer-tonic-design-system.git#web-dist",
    "cssesc": "^3.0.0",
    "deepmerge": "^4.3.1",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "gh-pages": "^6.0.0",
    "glob": "^10.2.2",
    "gulp": "^4.0.2",
    "minimist": "^1.2.8",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-js": "^4.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^11.2.2"
  },
  "peerDependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0"
  }
}
